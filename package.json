{
  "name": "@namchey/linkpreview",
  "version": "1.4.0",
  "description": "Open Graph, Twitter Card, Oembed Scrapper. Shows visual cards that mimics link previews in social media like facebook, twitter, vk and other sites that support link preview.",
  "repository": "https://github.com/namchey/linkpreview",
  "main": "server/index.js",
  "bin": {
    "linkpreview": "./bin/linkpreview"
  },
  "keywords": [
    "opengraph",
    "twittercard",
    "oembed",
    "scrape",
    "npm",
    "visual",
    "link",
    "unfurl",
    "react",
    "redux",
    "cheeriojs",
    "scraping"
  ],
  "scripts": {
    "pm2:start": "pm2 startOrRestart ecosystem.json --env production --name $APP_NAME -l app_logs.log",
    "pm2:restart": "pm2 stop $APP_NAME && pm2 delete $APP_NAME && npm run pm2:start",
    "pm2": "if pm2 list | grep $APP_NAME; then npm run pm2:restart; else npm run pm2:start; fi",
    "lint": "eslint app",
    "postinstall": "npm run build",
    "lint:fix": "eslint --ext .js,.jsx app server --fix",
    "clean": "./node_modules/rimraf/bin.js public/assets && ./node_modules/rimraf/bin.js public/server-assets",
    "start": "NODE_ENV=production node server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --colors --progress --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:prod",
    "app-dev-server": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --port 8181 --progress --colors --hot-only --config ./webpack/webpack.config.dev-client.js",
    "test:server": "cross-env NODE_ENV=test ./node_modules/nyc/bin/nyc.js --reporter=html --reporter=lcov --reporter=text-summary node_modules/mocha/bin/mocha --growl modules/**/*.tests.js --grep",
    "test": "cross-env NODE_ENV=test ./node_modules/karma/bin/karma start --ONLY",
    "test:watch": "cross-env  NODE_ENV=test npm test --watch -- --no-single-run --ONLY"
  },
  "nyc": {
    "exclude": [
      "modules/**/tests",
      "public",
      "webpack",
      "config"
    ]
  },
  "private": false,
